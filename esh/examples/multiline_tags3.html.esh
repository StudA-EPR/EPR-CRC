<!DOCTYPE html>
<html>
	<head>
		<title><% date +"%T" %></title>
	</head>
	<body>
		<h1>Dateien in /Users/nilssommer/Documents</h1>
		<ul>
		<% esh_begin %>
			image_filenames=(/Users/nilssommer/Documents/*)
			for filename in "${image_filenames[@]}"
			do
				echo "<li>$filename</li>"
			done
		<% esh_end %>
		</ul>
		<h2>Weitere Infos</h2>
		<% esh_begin %>
		echo "<strong>Aktueller Benutzer:</strong>"
		whoami
		echo "<strong>Systeminfos:</strong>"
		uname -a
		<% esh_end %>
		<p>abcdefg</p>
	</body>
</html>
